<div class="container">
    <h3 style="text-align:center"> TASK IT </h3>
    <div class="row">
        <div class="col-xs-8">
            <h5>Task Progress</h5>
            <progress class="progress progress-success" id="progressBar" value="{{prog}}" max="100"></progress>
        </div>
        <div class="col-xs-4">
            Your Points: {{taskData.lifecount.length}}
        </div>
    </div>
</div>
<hr>
<div class="col-xs-12 ">
    <!--Panel-->
   
    <!--/.Panel-->
    <div class="card text-xs-center">
        <div class="card-header" style="text-align:center">
            To Do Today 
        </div>
        {{#each taskData.cogs}}
        <div id="accordion" role="tablist" aria-multiselectable="true">
            <div class="card">
                <div class="card-header taskclick" role="tab" id="headingOne">
                    <h5 class="mb-0">
                    <a>{{task}} </a>
                     </h5>
                </div>
                <div id="collapseOne" class="collapse" role="tabpanel" aria-labelledby="headingOne">
                    <div class="card-block">
                        {{description}}
                    </div>
                </div>
            </div>
            {{/each}}
        </div>
    </div>
</div>
    <div id="friends" class="panel panel-default">
        <div class="panel-heading">
            <h5 class="panel-title" style="text-align: center;">Try to beat your friends!</h5>
        </div>
        <div class="panel-body">
            <div class="thumbnail">
                <a href="{{tryToBeat.link}}" style="text-align: center;">
                    <p> {{tryToBeat.name}} </p>
                    <div id="top-score">
                        <span> Beat their score of: {{tryToBeat.score}} </span>
                    </div>
                </a>
            </div>
        </div>
    </div>